`nameA` varchar(32) CHARACTER SET utf8mb4 NOT NULL,

`nameC` varchar(32) CHARACTER SET gbk COLLATE gbk_bin NOT NULL, 이거는
`nameC` varchar(32) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL, 이렇게 바꿔?


ALTER TABLE 테이블이름
MODIFY COLUMN nameA VARCHAR(32) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL,
MODIFY COLUMN nameC VARCHAR(32) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL;